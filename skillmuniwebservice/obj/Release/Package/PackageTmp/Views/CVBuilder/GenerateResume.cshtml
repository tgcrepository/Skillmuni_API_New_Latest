              @@media only screen and (max-width: 480px) {


                  .basicname {
                      font-family: "Arial Narrow","Britannic Bold", Arial, "Arial Black";
                      font-size: 20px;
                      padding: 10px;
                      font-weight: bold;
                      color: #FFF;
                  }

                  .basicaddress {
                      text-align: right;
                      line-height: 25px;
