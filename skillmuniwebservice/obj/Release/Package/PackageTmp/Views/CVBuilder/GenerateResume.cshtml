              table {
                  border-collapse: collapse !important;
              }

              .basicname {
                  font-family: "Arial Narrow","Britannic Bold", Arial, "Arial Black";
                  font-size: 50px;
                  padding: 40px;
                  font-weight: bold;
                  color: #FFF;
              }

              .basicaddress {
                  text-align: right;
                  line-height: 25px;
                  font-size: 17px;
                  padding: 40px;
                  color: #FFF;
              }

              .dotunderline {
                  border-bottom: 2px dotted #c0c0c0;
                  margin: 0px 10px 20px 15px;
              }

              .tdwidth70 {
                  width: 70% !important;
              }

              .tdwidth30 {
                  width: 30% !important;
              }

              .summaryh2 {
                  font-weight: bold;
                  color: #404040;
                  padding-left: 15px;
