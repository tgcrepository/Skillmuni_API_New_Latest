
@{
    ViewBag.Title = "GenerateResume";
    Layout = null;
    string Name = (string)ViewData["Name"];
    m2ostnextservice.Models.CreateResumeDetails CVMaster = (m2ostnextservice.Models.CreateResumeDetails)ViewData["CVMaster"];
    int i = 0;
}

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Untitled Document</title>
    <style>

              *, *:after, *:before {
                  -webkit-box-sizing: border-box;
