<body>


    <div class="landing-page">
        <div class="landing-page-inner">
            <div class="rowland">
                <div class="col s12">
                    <table width="100%" style="margin-top: 7px;">
                        <tbody style="background:#099;">
                            <tr>

                                <td width="50%"><p class="basicname">@CVMaster.personel.first_name <br>@CVMaster.personel.last_name</p></td>
                                <td>
                                    <p class="basicaddress">
                                        @CVMaster.personel.mobile<br>
                                        @CVMaster.personel.email<br>
                                        @CVMaster.personel.street<br>
                                        @CVMaster.personel.country<br>
                                        DOB: @CVMaster.personel.day/@CVMaster.personel.month/@CVMaster.personel.year
                                    </p>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>

                <div class="col s12">
                    <table>
                        <tbody>
                            <tr>
                                <td valign="top" class="tdwidth70">
                                    <table style="background:#FFFFFF;">
                                        <tr>
                                            <td>
                                                <h2 class="summaryh2">Summary</h2>
                                                <p class="summaryp">
                                                    @CVMaster.personel.about
                                                </p>
                                                <p class="dotunderline">&nbsp;</p>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h2 class="summaryh2">Education</h2>
                                                @foreach (var itm in CVMaster.education)
                                                {
                                                    <h2 class="summaryh2" style="font-size:16px;">@itm.college</h2>
                                                    <h3 class="summaryh2" style="color:#CCC;">@itm.start_date - @itm.end_date</h3>
                                                    <p class="summaryp">
                                                        @itm.degree<br>
                                                        @itm.summary
                                                    </p>
                                                }
                                                <p class="dotunderline">&nbsp;</p>
                                            </td>
                                        </tr>

                                        <tr>
                                            <td>
                                                <h2 class="summaryh2">Project</h2>
                                                @foreach (var itm in CVMaster.project_list)
                                                {
                                                    <h2 class="summaryh2" style="font-size:16px;">@itm.college</h2>
                                                    <h3 class="summaryh2" style="color:#CCC;">@itm.start_date - @itm.end_date</h3>
                                                    <p class="summaryp">
                                                        @itm.project_title<br>
                                                        @itm.summary
                                                    </p>
                                                }
                                                <p class="dotunderline">&nbsp;</p>
                                            </td>
                                        </tr>

                                        <tr>
                                            <td>
                                                <h2 class="summaryh2"></h2>
                                                <h2 class="summaryh2" style="font-size:16px;"></h2>
                                                <h3 class="summaryh2" style="color:#CCC;"></h3>
                                                <p class="summaryp">
                                                    <br>

                                                </p>
                                                <p class="dotunderline">&nbsp;</p>
                                            </td>
                                        </tr>

                                    </table>
                                </td>
                                <td valign="top" class="tdwidth30">
                                    <table style="background:#FFFFFF;">
                                        <tr>
                                            <td>
                                                <h2 class="summaryh2">Skills</h2>
                                                <p class="summaryp">@CVMaster.additional_info.skills</p>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h2 class="summaryh2">Languages</h2>
                                                <p class="summaryp">@CVMaster.additional_info.languages</p>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h2 class="summaryh2">Interest</h2>
                                                <p class="summaryp">@CVMaster.additional_info.intrests</p>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h2 class="summaryh2">Others</h2>
                                                <p class="summaryp">@CVMaster.additional_info.others</p>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h2 class="summaryh2">Awards</h2>
                                                <p class="summaryp">@CVMaster.additional_info.awards</p>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>

                                                <h2 class="summaryh2">Social Profile</h2>
                                                <p class="sociallink">
                                                    <span class="sociallinkin">LinkedIn:</span><br>
                                                    @CVMaster.additional_info.linkedin<br>
                                                    <span class="sociallinkin">Facebook:</span><br>
                                                    @CVMaster.additional_info.facebook<br>
                                                    <span class="sociallinkin">Twitter:</span><br>
                                                    @CVMaster.additional_info.twitter
                                                </p>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!--<div class="col s12">
                    <table style="background:#9C3; margin-bottom:70px;">
                        <tr>
                            <td>dfgdfgdf</td>
                        </tr>
                    </table>
                </div>-->

            </div>
        </div>
    </div>
    <div class="landing-page landing-page-footer ">
        <p class="footerclass">E-mail: @CVMaster.personel.email</p>
    </div>

</body>
</html>
