                      color: #FFF;
                  }

                  .dotunderline {
                      border-bottom: 2px dotted #c0c0c0;
                      margin: 0px 0px 0px 0px;
                  }

                  .tdwidth70 {
                      width: 60% !important;
                  }

                  .tdwidth30 {
                      width: 40% !important;
                  }

                  .summaryh2 {
                      font-weight: bold;
                      color: #404040;
                      padding-left: 5px;
                      font-size: 15px;
                  }

                  .summaryp {
                      font-size: 12px;
                      color: #000;
                      padding-left: 5px;
                      line-height: 20px;
                      padding-right: 5px;
                  }

                  .sociallink {
                      font-size: 12px;
                      color: #000;
                      line-height: 22px;
                      margin-left: 5px;
                  }

                  .sociallinkin {
                      color: #c0c0c0;
                      font-size: 13px;
                      font-weight: bold;
                  }

                  .landing-page-footer {
                      background: #099;
                      margin-bottom: 0px;
                  }

                  .footerclass {
